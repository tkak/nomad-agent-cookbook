[Unit]
Description=<%= @name %>
Wants=network.target
After=network.target

[Service]
ExecStart=<%= @command %>
ExecReload=/bin/kill -<%= @reload_signal %> $MAINPID
KillSignal=<%= @stop_signal %>
User=<%= @user %>
WorkingDirectory=<%= @directory %>
LimitNOFILE=65536

[Install]
WantedBy=multi-user.target
